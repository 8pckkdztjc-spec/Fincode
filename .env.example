# ================================
# FinCode 环境变量配置模板
# ================================

# 推理模式: api (默认) 或 local (私有化部署)
INFERENCE_MODE=api

# DeepSeek API 配置
DEEPSEEK_API_KEY=your_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com

# 数据库配置
DATABASE_URL=postgresql://fincode:fincode@localhost:5432/fincode

# 应用配置
APP_ENV=development
DEBUG=true
SECRET_KEY=your_secret_key_here

# 日志级别
LOG_LEVEL=INFO

# 前端配置
VITE_API_BASE_URL=http://localhost:8000/api/v1
